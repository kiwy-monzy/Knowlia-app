{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WebrtcActive",
  "version": "0.1.0",
  "identifier": "WebrtcActive",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1424",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "WebrtcActive",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": ["./assets/cer.pem", "./assets/key.pem"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "dev": {
      "watch": true
    }
  }
}
